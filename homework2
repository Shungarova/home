n = int(input("Введите целое число: "))
if n > 0 and n%2==0:
    print("Положительное целое число")
elif n > 0 and n%2!=0:
    print("Положительное нецелое число")
elif n < 0 and n%2==0:
    print("Отрицательное целое число")
elif n < 0 and n % 2 != 0:
    print("Отрицательное нецелое число")
else:
    print("Ноль")
